{
  "name": "freee/socialite-freee",
  "description": "Socialite for Freee",
  "keywords": [
    "freee",
    "socialite",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2||^8.0",
    "ext-json": "*",
    "laravel/socialite": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Freee\\Socialite\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Freee\\Socialite\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Freee\\Socialite\\FreeeServiceProvider"
      ]
    }
  }
}
