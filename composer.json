{
    "name": "freee/freee-accounting-sdk",
    "version": "v2.6.0",
    "description": "Accounting freee PHP SDK",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "freee"
    ],
    "homepage": "https://developer.freee.co.jp/",
    "license": "MIT",
    "authors": [
        {
            "name": "Freee",
            "homepage": "https://developer.freee.co.jp/"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Freee\\Accounting\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freee\\Accounting\\Test\\": "test/"
        }
    },
    "type": "library"
}
